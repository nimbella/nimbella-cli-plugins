
/* {{{description}}} */

function main(params) {
    return {
        statusCode: {{code}},
        {{#if headers}}
        headers: {
        {{#each headers}}
        "{{{this.key}}}":"{{{this.value}}}",
        {{/each}}
        },
        {{/if}}
        {{#if body}}
        body: {{{body}}},
        {{else}}
        body: '',
        {{/if}}
    };
}

exports.main = main;
